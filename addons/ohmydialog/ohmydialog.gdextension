[configuration]

entry_symbol = "ohmydialog_library_init"
compatibility_minimum = "4.5"
reloadable = true

[libraries]

; Windows
windows.editor.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.windows.editor.x86_64.dll"
windows.template_debug.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.windows.template_debug.x86_64.dll"
windows.template_release.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.windows.template_release.x86_64.dll"

; Linux
linux.editor.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.linux.editor.x86_64.so"
linux.template_debug.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.linux.template_debug.x86_64.so"
linux.template_release.x86_64 = "res://addons/ohmydialog/gdextension/libohmydialog.linux.template_release.x86_64.so"

; macOS
macos.editor = "res://addons/ohmydialog/gdextension/libohmydialog.macos.editor.framework"
macos.template_debug = "res://addons/ohmydialog/gdextension/libohmydialog.macos.template_debug.framework"
macos.template_release = "res://addons/ohmydialog/gdextension/libohmydialog.macos.template_release.framework"
